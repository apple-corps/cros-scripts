# We moved a lot of files to the public overlay.  Make sure we regen the
# boards profile settings and such so we pull in the public bits.
BOARD=peppy

if [[ -d /build/${BOARD} ]]; then
  ~/trunk/src/scripts/setup_board \
    --board=${BOARD} \
    --skip_chroot_upgrade \
    --regen_configs
fi
