# Regen boards profile settings and such after giving arm-generic a profile
BOARD=arm-generic

if [[ -d /build/${BOARD} ]]; then
  ~/trunk/src/scripts/setup_board \
    --board=${BOARD} \
    --skip_chroot_upgrade \
    --regen_configs
fi

BOARD=arm64-generic

if [[ -d /build/${BOARD} ]]; then
  ~/trunk/src/scripts/setup_board \
    --board=${BOARD} \
    --skip_chroot_upgrade \
    --regen_configs
fi
